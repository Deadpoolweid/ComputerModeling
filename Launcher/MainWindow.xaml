<Window x:Class="Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Launcher"
        mc:Ignorable="d"
        Title="Launcher" Height="157.895" Width="403.947">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="10,53,0,0" VerticalAlignment="Top" Width="202" Height="31">
            <ComboBoxItem Content="Аддитивная модель" HorizontalAlignment="Left" Width="200"/>
            <ComboBoxItem Content="Мультипликативная модель" HorizontalAlignment="Left" Width="200"/>
            <ComboBoxItem Content="Генерация случайных чисел" HorizontalAlignment="Left" Width="200"/>
        </ComboBox>
        <Button x:Name="button" Content="Запуск" HorizontalAlignment="Left" Margin="246,53,0,0" VerticalAlignment="Top" Width="140" Height="31" Click="button_Click"/>

    </Grid>
</Window>
